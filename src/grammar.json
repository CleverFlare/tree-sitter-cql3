{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "cql3",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_statement"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ";"
              },
              {
                "type": "SYMBOL",
                "name": "_statement"
              }
            ]
          }
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "create_table_statement"
        }
      ]
    },
    "create_table_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "create",
          "content": {
            "type": "SYMBOL",
            "name": "kw_create"
          }
        },
        {
          "type": "FIELD",
          "name": "table",
          "content": {
            "type": "SYMBOL",
            "name": "kw_table"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "if_not_exists"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "table_name",
          "content": {
            "type": "SYMBOL",
            "name": "table_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "column_definitions"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "with_clause"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "if_not_exists": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_if"
        },
        {
          "type": "SYMBOL",
          "name": "kw_not"
        },
        {
          "type": "SYMBOL",
          "name": "kw_exists"
        }
      ]
    },
    "table_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "identifier"
                },
                {
                  "type": "STRING",
                  "value": "."
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "column_definitions": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "column_definition"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "column_definition"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "column_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "data_types"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "primary_key_inline"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "primary_key_inline": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_primary"
        },
        {
          "type": "SYMBOL",
          "name": "kw_key"
        }
      ]
    },
    "data_types": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[iI][nN][tT]"
        },
        {
          "type": "PATTERN",
          "value": "[tT][eE][xX][tT]"
        },
        {
          "type": "PATTERN",
          "value": "[uU][uU][iI][dD]"
        },
        {
          "type": "PATTERN",
          "value": "[bB][oO][oO][lL][eE][aA][nN]"
        },
        {
          "type": "PATTERN",
          "value": "[tT][iI][mM][eE][sS][tT][aA][mM][pP]"
        }
      ]
    },
    "with_clause": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_with"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "table_option"
          }
        }
      ]
    },
    "table_option": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        }
      ]
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "string": {
      "type": "PATTERN",
      "value": "'[^']*'"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "--"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    },
    "kw_create": {
      "type": "PATTERN",
      "value": "[cC][rR][eE][aA][tT][eE]"
    },
    "kw_table": {
      "type": "PATTERN",
      "value": "[tT][aA][bB][lL][eE]"
    },
    "kw_if": {
      "type": "PATTERN",
      "value": "[iI][fF]"
    },
    "kw_not": {
      "type": "PATTERN",
      "value": "[nN][oO][tT]"
    },
    "kw_exists": {
      "type": "PATTERN",
      "value": "[nN][oO][tT]"
    },
    "kw_primary": {
      "type": "PATTERN",
      "value": "[pP][rR][iI][mM][aA][rR][yY]"
    },
    "kw_key": {
      "type": "PATTERN",
      "value": "[kK][eE][yY]"
    },
    "kw_with": {
      "type": "PATTERN",
      "value": "[wW][iI][tT][hH]"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}