  "grammars": [
    {
      "name": "cql3",
      "camelcase": "Cql3",
      "title": "CQL3",
      "scope": "source.cql3",
      "file-types": [
        "cql"
      ],
      "injection-regex": "^cql3$",
      "class-name": "TreeSitterCql3"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter grammar for CQL3",
    "authors": [
      {
        "name": "Muhammad maher",
        "email": "blastclever@gmail.com",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/CleverFlare/tree-sitter-cql3.git",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
